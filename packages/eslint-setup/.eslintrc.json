{
    "env": {
        "browser": true,
        "es2021": true
    },
    "root": true,
    "extends": ["eslint:recommended","airbnb-base"],
    "plugins": ["promise"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "noInlineConfig": false,
    "rules": {
        "promise/catch-or-return": "error",
        "semi":[2,"never"]
    }
}
