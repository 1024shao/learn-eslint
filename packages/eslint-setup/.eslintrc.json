{
    "env": {
        "browser": true,
        "es2021": true
    },
    "root": true,
    "extends": "eslint:recommended",
    "plugins": ["promise"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "noInlineConfig": false,
    "rules": {
        "promise/catch-or-return": "error"
    }
}
